{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Not_Mini 200 Subs Texture Pack",
  "version": "1.0.0",
  "description": "A browser extension to replace MiniBlox tools with custom textures for 200 subs!",
  "author": "Not_Mini",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://miniblox.io/*",
    "https://miniblox.online/*",
    "https://raw.githubusercontent.com/*"
  ]
}
